@{
    ViewData["Title"] = "Contato";
}


    <div class="container contato">
        <form>
            <div class="form-group">
              <label for="exampleInputEmail1">Endereço de email:</label>
              <input type="email" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Seu email">
              <small id="emailHelp" class="form-text text-white">Nunca vamos compartilhar seu email, com ninguém.</small>
            </div>
            <div class="form-group">
                <label for="exampleFormControlTextarea1">Digite sua mensagem:</label>
                <textarea class="form-control" id="exampleFormControlTextarea1" rows="3" placeholder="Digite sua mensagem..."></textarea>
              </div>
          
            <button type="submit" class="btn btn-contato">Enviar</button>
          </form>
    </div>
    
    
       


